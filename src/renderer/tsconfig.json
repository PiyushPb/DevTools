{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "esnext"],
    "jsx": "react-jsx", // Ensure that JSX is enabled
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true
  },
  "include": [
    "src/**/*" // Include your source files
  ],
  "exclude": [
    "node_modules" // Exclude node_modules
  ]
}
